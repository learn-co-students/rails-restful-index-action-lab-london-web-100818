<h1>Student Index</h1>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th></th>
    </tr>

    <%@students.each do |s|%>
        <tr>
            <td><%=s.first_name%></td>
            <td><%=s.last_name%></td>
            <td></td>
        </tr>
    <%end%>
</table>